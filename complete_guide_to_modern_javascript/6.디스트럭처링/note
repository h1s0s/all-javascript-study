//6.1 객체 디스트럭처링

//객체에서 변수를 생성하려면?

var person = {
    first: "han",
    last: "mon"
}
var first = person.first

const {first, last} = person;

//디스트럭처링을 이용하여 person이 가진 속성에 접근함과 동시에 해당 속성 이름으로 변수선언이 가능함.

//다음과 같이 중첩된 객체 형태도 가능

const person = {
    name: "han",
    last: "mon",
    links: {
        social: {
            facebook: "a"
        },
        website: "b"
    }
}

const {facebook} = person.links.social;

// 변수이름을 바꿀수도 있음
const {facebook: fb} = person.links.social;

// 기본 값을 전달할수도 이씀

const {facebook: ff = "A"} = person.links.social;


//6.2 배열 디스트럭처링

const person = ["A", "B", 10];
const [name, lastname, age] = person;


const [name2, ...another] = person;
//another에 나머지 값들이 배열형태로 들어감.
